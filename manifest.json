{    "manifest_version": 3,
    "name": "FormFill",
    "version": "0.3",
    "description": "Refill Google Form",
    "action": {
        "default_title": "Form Filler",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches": ["http://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "offline_enabled": false,
    "permissions": [
        "webRequest",
        "storage"
    ],
    "icons": {
        "512": "512x512.png"
    },
	"host_permissions": [
		"<all_urls>"
	]
}