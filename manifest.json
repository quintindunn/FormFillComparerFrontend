{    "manifest_version": 3,
    "name": "FormFillComparer",
    "version": "0.3",
    "description": "Refill and compare Flubaroo Submissions",
    "action": {
        "default_title": "Form Fill Comparer",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
        "matches": ["http://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "offline_enabled": false,
    "permissions": [
        "webRequest",
        "webNavigation",
        "storage",
        "activeTab",
        "tabs"
    ],
    "icons": {
    
    },
	"host_permissions": [
		"<all_urls>"
	]
}